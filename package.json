{
  "name": "extensio",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "concurrently --kill-others \"npm run serve\" \"electron .\"",
    "serve": "vue-cli-service serve",
    "buildOnly": "vue-cli-service build && shx cp index.js dist/index.js",
    "build": "npm run buildOnly && cd dist && concurrently --kill-others \"npx serve -l 8080\" \"wait-on http://localhost:8080 && electron index.js\"",
    "test:unit": "vue-cli-service test:unit"
  },
  "dependencies": {
    "autoprefixer": "^10",
    "concurrently": "^8.2.2",
    "core-js": "^3.8.3",
    "express": "^4.18.3",
    "postcss": "^8",
    "register-service-worker": "^1.7.2",
    "vue": "^3.2.13",
    "vue-router": "^4.0.3",
    "wait-on": "^7.2.0"
  },
  "devDependencies": {
    "@types/jest": "^27.0.1",
    "@typescript-eslint/eslint-plugin": "^5.4.0",
    "@typescript-eslint/parser": "^5.4.0",
    "@vue/cli-plugin-babel": "~5.0.0",
    "@vue/cli-plugin-eslint": "~5.0.0",
    "@vue/cli-plugin-pwa": "~5.0.0",
    "@vue/cli-plugin-router": "~5.0.0",
    "@vue/cli-plugin-typescript": "~5.0.0",
    "@vue/cli-plugin-unit-jest": "~5.0.0",
    "@vue/cli-service": "~5.0.0",
    "@vue/eslint-config-typescript": "^9.1.0",
    "@vue/test-utils": "^2.0.0-0",
    "@vue/vue3-jest": "^27.0.0-alpha.1",
    "babel-jest": "^27.0.6",
    "electron": "^29.1.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-vue": "^8.0.3",
    "jest": "^27.0.5",
    "less": "^4.0.0",
    "less-loader": "^8.0.0",
    "lint-staged": "^11.1.2",
    "prettier": "^2.4.1",
    "shx": "^0.3.4",
    "ts-jest": "^27.0.4",
    "typescript": "~4.5.5",
    "vue-cli-plugin-tailwind": "~3.0.0"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  }
}
